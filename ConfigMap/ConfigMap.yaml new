apiVersion: v1
kind: Pod
metadata:
  creationTimestamp: null
  labels:
    run: firstpod
  name: firstpod
spec:
  containers:
  - image: nithya890/first:1.0
    name: firstpod
    ports:
    - containerPort: 8080
    resources: {}
    env:
      - name: DBHOST
        valueFrom:
          configMapKeyRef:
            name: mycm
            key: dbhost
      - name: DBPORT
        valueFrom:
          configMapKeyRef:
            name: mycm
            key: dbport
  dnsPolicy: ClusterFirst
  restartPolicy: Always
status: {}
